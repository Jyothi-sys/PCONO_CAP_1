<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core">


  <SelectDialog  
  noDataText="{i18n>NoDataFound}"
  title="{i18n>Days}"
  liveChange="onSearchDays"
  confirm="onSelectionDays"
  cancel="onDialogClose"
  multiSelect="true"
  items="{DaysData>/}">

<StandardListItem 

title="{path: 'DaysData>Days'}"
iconDensityAware="false"
iconInset="false"
type="Active" />


</SelectDialog >
</c:FragmentDefinition>

