<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core">


  <SelectDialog  
  noDataText="{i18n>NoDataFound}"
  title="{i18n>SalesOrder}"
  liveChange="onSearchSalesOrder"
  confirm="onSelectionSalesOrder"
  cancel="onDialogClose"
  multiSelect="true"
  items="{salesOrderData>/}">

<StandardListItem 

title="{path: 'salesOrderData>salesOrder'}"
iconDensityAware="false"
iconInset="false"
type="Active" />


</SelectDialog >
</c:FragmentDefinition>

