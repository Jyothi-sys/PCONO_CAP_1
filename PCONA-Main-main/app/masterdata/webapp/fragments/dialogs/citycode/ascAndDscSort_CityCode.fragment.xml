<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog 
		confirm="CityCode_handleSortDialogConfirm">
		<sortItems  >
			
			<ViewSettingsItem text="{i18n>city_code}" key="city_code" />
			<ViewSettingsItem text="{i18n>lang}" key="lang" />
			<ViewSettingsItem text="{i18n>area_code}" key="area_code" />
			<ViewSettingsItem text="{i18n>country_code}" key="country_code" />
			<ViewSettingsItem text="{i18n>city_name}" key="city_name" />
			<ViewSettingsItem text="{i18n>short_name}" key="short_name" />
			<ViewSettingsItem text="{i18n>created_on}" key="created_on" />
			<ViewSettingsItem text="{i18n>created_time}" key="created_time" />
			<ViewSettingsItem text="{i18n>changed_on}" key="changed_on" />
			<ViewSettingsItem text="{i18n>change_time}" key="change_time" />
			<ViewSettingsItem text="{i18n>legacy_information}" key="legacy_information"  />
			<ViewSettingsItem text="{i18n>sap_city}" key="sap_city"  />
			<ViewSettingsItem text="{i18n>created_by}" key="created_by" />						
			<ViewSettingsItem text="{i18n>changed_by}" key="changed_by" />
			
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>